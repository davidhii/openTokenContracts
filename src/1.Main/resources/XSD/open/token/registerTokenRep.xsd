<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://open/token/dto">

    <xs:element name="registerTokenRep">
        <xs:complexType>
            <xs:sequence>
                <!-- if resultCode is zero, there is no errorMessage, the system has updated the user profile -->
                <xs:element name="resultCode"   type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="errorMessage" type="xs:string" minOccurs="0" maxOccurs="1"/>

            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>
